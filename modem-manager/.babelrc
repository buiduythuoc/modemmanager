{
	"plugins": [
		[
			"module-resolver",
			{
				"cwd": "babelrc",
				"root": ["./src"],
				"extensions":[".js",".ios.js",".android.js"],
				"alias": {
					"@assets":"./src/assets",
					"@components":"./src/components",
					"@atoms":"./src/components/atoms",
					"@navigation":"./src/navigation",
					"@scenes":"./src/scenes",
					"@services":"./src/services",
					"@themes":"./src/themes",
					"@utils":"./src/utils"
				}
			}
		]
	]
}